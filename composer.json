{
  "name": "demoniacdeath/aop-bundle",
  "description": "Adds AOP capabilities to Symfony2",
  "keywords": [
    "annotations",
    "aop"
  ],
  "type": "symfony-bundle",
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "Johannes M. Schmitt",
      "email": "schmittjoh@gmail.com"
    },
    {
      "name": "Michael Skvortsov",
      "email": "demoniac.death@gmail.com"
    }
  ],
  "require": {
    "php": "^7.4",
    "symfony/framework-bundle": "4.4.*",
    "demoniacdeath/cg": "dev-master"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.0"
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/applandinc/appmap-php-cg"
    }
  ],
  "autoload": {
    "psr-4": {
      "JMS\\AopBundle\\": ""
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "2.0.x-dev"
    }
  }
}
